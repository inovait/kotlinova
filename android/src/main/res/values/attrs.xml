<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FullScreenProgressBar">
        <attr name="visibleAtStart" format="boolean" />
    </declare-styleable>
    <declare-styleable name="AspectRatioImageView">
        <attr name="fixedMultiplier" format="float" />
        <attr name="fixedDimension" format="enum">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="CoveredSwipeMenuView">
        <attr name="leftItem" format="reference"/>
        <attr name="rightItem" format="reference"/>
        <attr name="swipingItem" format="reference"/>
        <attr name="behindBackground" format="reference"/>
    </declare-styleable>
</resources>